{
  "name": "shoez-store-server",
  "version": "1.0.0",
  "description": "apollo server for shoez-store website",
  "main": "./dist/index.js",
  "type": "module",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thisisaarush/ShoezStore-Backend.git"
  },
  "scripts": {
    "compile": "tsc",
    "dev": "nodemon ./src/index.ts",
    "start": "npm run compile && node ./dist/src/index.js",
    "prisma-pull": "npx prisma db pull --force && prisma generate",
    "prisma-push": "npx prisma db push",
    "prisma-gen": "prisma generate",
    "seed": "npx prisma db seed"
  },
  "keywords": [
    "e-commerce",
    "shoes",
    "shoes online",
    "buy shoes",
    "shoes store"
  ],
  "author": "Aarush Tanwar",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@prisma/client": "^4.8.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
